{
  "name": "calculator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "npm run build:sass && react-scripts start && npm run watch:sass",
    "build": "npm run build:sass && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:sass": "npm run build:sass && npm run build:sass -- -w",
    "build:sass": "node-sass -r --output-style compressed src/stylesheets/main.scss -o src/stylesheets/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
